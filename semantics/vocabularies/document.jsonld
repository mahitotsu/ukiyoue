{
  "@context": {
    "@version": 1.1,
    "@vocab": "https://ukiyoue.dev/vocab#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "http://schema.org/",
    "dc": "http://purl.org/dc/terms/"
  },
  "@graph": [
    {
      "@id": "ukiyoue:Document",
      "@type": "rdfs:Class",
      "rdfs:label": "Document",
      "rdfs:comment": "Base class for all Ukiyoue framework documents",
      "rdfs:subClassOf": "schema:CreativeWork"
    },
    {
      "@id": "ukiyoue:BusinessRequirementsDocument",
      "@type": "rdfs:Class",
      "rdfs:label": "Business Requirements Document",
      "rdfs:comment": "Document containing business requirements, stakeholders, and use cases",
      "rdfs:subClassOf": "ukiyoue:Document"
    },
    {
      "@id": "ukiyoue:Stakeholder",
      "@type": "rdfs:Class",
      "rdfs:label": "Stakeholder",
      "rdfs:comment": "A person, group, or organization with interest in a project",
      "rdfs:subClassOf": "schema:Person"
    },
    {
      "@id": "ukiyoue:Requirement",
      "@type": "rdfs:Class",
      "rdfs:label": "Requirement",
      "rdfs:comment": "A specified capability or constraint that a system must satisfy"
    },
    {
      "@id": "ukiyoue:FunctionalRequirement",
      "@type": "rdfs:Class",
      "rdfs:label": "Functional Requirement",
      "rdfs:comment": "A requirement that specifies what the system should do",
      "rdfs:subClassOf": "ukiyoue:Requirement"
    },
    {
      "@id": "ukiyoue:NonFunctionalRequirement",
      "@type": "rdfs:Class",
      "rdfs:label": "Non-Functional Requirement",
      "rdfs:comment": "A requirement that specifies how the system should perform",
      "rdfs:subClassOf": "ukiyoue:Requirement"
    },
    {
      "@id": "ukiyoue:UseCase",
      "@type": "rdfs:Class",
      "rdfs:label": "Use Case",
      "rdfs:comment": "A description of how a system interacts with actors to achieve a goal"
    },
    {
      "@id": "ukiyoue:ProjectOverview",
      "@type": "rdfs:Class",
      "rdfs:label": "Project Overview",
      "rdfs:comment": "High-level information about a project including background, objectives, and scope"
    },
    {
      "@id": "ukiyoue:projectOverview",
      "@type": "rdf:Property",
      "rdfs:label": "project overview",
      "rdfs:comment": "Project overview information",
      "rdfs:domain": "ukiyoue:BusinessRequirementsDocument",
      "rdfs:range": "ukiyoue:ProjectOverview"
    },
    {
      "@id": "ukiyoue:stakeholder",
      "@type": "rdf:Property",
      "rdfs:label": "stakeholder",
      "rdfs:comment": "A stakeholder associated with the document",
      "rdfs:domain": "ukiyoue:Document",
      "rdfs:range": "ukiyoue:Stakeholder"
    },
    {
      "@id": "ukiyoue:useCase",
      "@type": "rdf:Property",
      "rdfs:label": "use case",
      "rdfs:comment": "A use case defined in the document",
      "rdfs:domain": "ukiyoue:BusinessRequirementsDocument",
      "rdfs:range": "ukiyoue:UseCase"
    },
    {
      "@id": "ukiyoue:functionalRequirement",
      "@type": "rdf:Property",
      "rdfs:label": "functional requirement",
      "rdfs:comment": "A functional requirement specified in the document",
      "rdfs:domain": "ukiyoue:BusinessRequirementsDocument",
      "rdfs:range": "ukiyoue:FunctionalRequirement"
    },
    {
      "@id": "ukiyoue:nonFunctionalRequirement",
      "@type": "rdf:Property",
      "rdfs:label": "non-functional requirement",
      "rdfs:comment": "A non-functional requirement specified in the document",
      "rdfs:domain": "ukiyoue:BusinessRequirementsDocument",
      "rdfs:range": "ukiyoue:NonFunctionalRequirement"
    },
    {
      "@id": "ukiyoue:need",
      "@type": "rdf:Property",
      "rdfs:label": "need",
      "rdfs:comment": "A need expressed by a stakeholder",
      "rdfs:domain": "ukiyoue:Stakeholder",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:successCriterion",
      "@type": "rdf:Property",
      "rdfs:label": "success criterion",
      "rdfs:comment": "A criterion for measuring stakeholder satisfaction",
      "rdfs:domain": "ukiyoue:Stakeholder",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:priorityLevel",
      "@type": "rdf:Property",
      "rdfs:label": "priority level",
      "rdfs:comment": "Priority level (critical, high, medium, low)",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:influenceLevel",
      "@type": "rdf:Property",
      "rdfs:label": "influence level",
      "rdfs:comment": "Level of influence (high, medium, low)",
      "rdfs:domain": "ukiyoue:Stakeholder",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:interestLevel",
      "@type": "rdf:Property",
      "rdfs:label": "interest level",
      "rdfs:comment": "Level of interest (high, medium, low)",
      "rdfs:domain": "ukiyoue:Stakeholder",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:acceptanceCriterion",
      "@type": "rdf:Property",
      "rdfs:label": "acceptance criterion",
      "rdfs:comment": "A testable criterion for requirement acceptance",
      "rdfs:domain": "ukiyoue:Requirement",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:requirementStatus",
      "@type": "rdf:Property",
      "rdfs:label": "requirement status",
      "rdfs:comment": "Current status of a requirement (proposed, approved, in-progress, implemented, tested, deployed, rejected)",
      "rdfs:domain": "ukiyoue:Requirement",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:requirementCategory",
      "@type": "rdf:Property",
      "rdfs:label": "requirement category",
      "rdfs:comment": "Category or domain of a requirement",
      "rdfs:domain": "ukiyoue:Requirement",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:requestedBy",
      "@type": "rdf:Property",
      "rdfs:label": "requested by",
      "rdfs:comment": "Stakeholder who requested this requirement",
      "rdfs:domain": "ukiyoue:Requirement",
      "rdfs:range": "ukiyoue:Stakeholder"
    },
    {
      "@id": "ukiyoue:primaryActor",
      "@type": "rdf:Property",
      "rdfs:label": "primary actor",
      "rdfs:comment": "The primary actor in a use case",
      "rdfs:domain": "ukiyoue:UseCase",
      "rdfs:range": "ukiyoue:Stakeholder"
    },
    {
      "@id": "ukiyoue:precondition",
      "@type": "rdf:Property",
      "rdfs:label": "precondition",
      "rdfs:comment": "A condition that must be true before use case execution",
      "rdfs:domain": "ukiyoue:UseCase",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:postcondition",
      "@type": "rdf:Property",
      "rdfs:label": "postcondition",
      "rdfs:comment": "A condition that must be true after successful use case execution",
      "rdfs:domain": "ukiyoue:UseCase",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:mainFlow",
      "@type": "rdf:Property",
      "rdfs:label": "main flow",
      "rdfs:comment": "Main success scenario of a use case",
      "rdfs:domain": "ukiyoue:UseCase"
    },
    {
      "@id": "ukiyoue:alternativeFlow",
      "@type": "rdf:Property",
      "rdfs:label": "alternative flow",
      "rdfs:comment": "An alternative path in a use case",
      "rdfs:domain": "ukiyoue:UseCase"
    },
    {
      "@id": "ukiyoue:exceptionFlow",
      "@type": "rdf:Property",
      "rdfs:label": "exception flow",
      "rdfs:comment": "An exception handling flow in a use case",
      "rdfs:domain": "ukiyoue:UseCase"
    },
    {
      "@id": "ukiyoue:addresses",
      "@type": "rdf:Property",
      "rdfs:label": "addresses",
      "rdfs:comment": "Requirement that this use case addresses",
      "rdfs:domain": "ukiyoue:UseCase",
      "rdfs:range": "ukiyoue:Requirement"
    },
    {
      "@id": "ukiyoue:executionFrequency",
      "@type": "rdf:Property",
      "rdfs:label": "execution frequency",
      "rdfs:comment": "Expected frequency of use case execution",
      "rdfs:domain": "ukiyoue:UseCase",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:documentStatus",
      "@type": "rdf:Property",
      "rdfs:label": "document status",
      "rdfs:comment": "Current status of the document (draft, review, approved, published, deprecated, archived)",
      "rdfs:domain": "ukiyoue:Document",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:confidentialityLevel",
      "@type": "rdf:Property",
      "rdfs:label": "confidentiality level",
      "rdfs:comment": "Confidentiality level of the document (public, internal, confidential, restricted)",
      "rdfs:domain": "ukiyoue:Document",
      "rdfs:range": "rdfs:Literal"
    },
    {
      "@id": "ukiyoue:glossaryTerm",
      "@type": "rdf:Property",
      "rdfs:label": "glossary term",
      "rdfs:comment": "A term defined in the project glossary",
      "rdfs:domain": "ukiyoue:Document"
    }
  ]
}
