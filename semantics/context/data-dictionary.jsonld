{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "ukiyoue": "https://ukiyoue.example.org/vocab#",
    "dcat": "http://www.w3.org/ns/dcat#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "prov": "http://www.w3.org/ns/prov#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "DataDictionary": {
      "@id": "ukiyoue:DataDictionary",
      "@type": "@id",
      "@context": {
        "terms": {
          "@id": "dcat:Dataset",
          "@container": "@set"
        }
      }
    },
    "Term": {
      "@id": "ukiyoue:Term",
      "@type": "@id"
    },
    "termName": {
      "@id": "skos:prefLabel",
      "@type": "xsd:string"
    },
    "canonicalName": {
      "@id": "ukiyoue:canonicalName",
      "@type": "xsd:string"
    },
    "synonyms": {
      "@id": "skos:altLabel",
      "@type": "xsd:string",
      "@container": "@set"
    },
    "definition": {
      "@id": "skos:definition",
      "@type": "xsd:string"
    },
    "dataType": {
      "@id": "ukiyoue:dataType",
      "@type": "xsd:string"
    },
    "constraints": {
      "@id": "ukiyoue:constraints",
      "@type": "@id"
    },
    "domain": {
      "@id": "ukiyoue:domain",
      "@type": "@vocab",
      "@context": {
        "business": "ukiyoue:BusinessDomain",
        "system": "ukiyoue:SystemDomain",
        "analytics": "ukiyoue:AnalyticsDomain",
        "infrastructure": "ukiyoue:InfrastructureDomain"
      }
    },
    "layer": {
      "@id": "ukiyoue:layer",
      "@type": "@vocab",
      "@context": {
        "conceptual": "ukiyoue:ConceptualLayer",
        "logical": "ukiyoue:LogicalLayer",
        "physical": "ukiyoue:PhysicalLayer"
      }
    },
    "deprecated": {
      "@id": "ukiyoue:deprecated",
      "@type": "xsd:boolean"
    },
    "replacedBy": {
      "@id": "prov:wasRevisedBy",
      "@type": "@id"
    },
    "tags": {
      "@id": "dcat:keyword",
      "@type": "xsd:string",
      "@container": "@set"
    },
    "termReference": {
      "@id": "prov:wasDerivedFrom",
      "@type": "@id"
    },
    "relatedTerms": {
      "@id": "skos:related",
      "@type": "@id",
      "@container": "@set"
    },
    "broaderTerm": {
      "@id": "skos:broader",
      "@type": "@id"
    },
    "narrowerTerm": {
      "@id": "skos:narrower",
      "@type": "@id"
    },
    "inScheme": {
      "@id": "skos:inScheme",
      "@type": "@id"
    },
    "conceptScheme": {
      "@id": "skos:ConceptScheme",
      "@type": "@id"
    },
    "example": {
      "@id": "skos:example",
      "@type": "xsd:string"
    },
    "note": {
      "@id": "skos:note",
      "@type": "xsd:string"
    },
    "changeNote": {
      "@id": "skos:changeNote",
      "@type": "xsd:string"
    },
    "historyNote": {
      "@id": "skos:historyNote",
      "@type": "xsd:string"
    },
    "scopeNote": {
      "@id": "skos:scopeNote",
      "@type": "xsd:string"
    }
  }
}
