{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "@vocab": "https://ukiyoue.dev/vocab#",
    "schema": "http://schema.org/",
    "dc": "http://purl.org/dc/terms/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "id": "@id",
    "type": "@type",
    "Document": {
      "@id": "schema:CreativeWork",
      "@context": {
        "title": "schema:name",
        "description": "schema:description",
        "authors": {
          "@id": "schema:author",
          "@container": "@set",
          "@context": {
            "name": "schema:name",
            "email": "schema:email",
            "role": "schema:roleName"
          }
        },
        "created": {
          "@id": "schema:dateCreated",
          "@type": "xsd:dateTime"
        },
        "modified": {
          "@id": "schema:dateModified",
          "@type": "xsd:dateTime"
        },
        "version": "schema:version",
        "status": "ukiyoue:documentStatus",
        "language": "schema:inLanguage",
        "tags": {
          "@id": "schema:keywords",
          "@container": "@set"
        }
      }
    },
    "BusinessRequirements": {
      "@id": "ukiyoue:BusinessRequirementsDocument",
      "@context": {
        "projectOverview": "ukiyoue:projectOverview",
        "stakeholders": {
          "@id": "ukiyoue:stakeholder",
          "@container": "@set"
        },
        "useCases": {
          "@id": "ukiyoue:useCase",
          "@container": "@set"
        },
        "functionalRequirements": {
          "@id": "ukiyoue:functionalRequirement",
          "@container": "@set"
        },
        "nonFunctionalRequirements": {
          "@id": "ukiyoue:nonFunctionalRequirement",
          "@container": "@set"
        },
        "glossary": {
          "@id": "ukiyoue:glossaryTerm",
          "@container": "@set"
        }
      }
    },
    "Stakeholder": {
      "@id": "ukiyoue:Stakeholder",
      "@context": {
        "name": "schema:name",
        "role": "schema:roleName",
        "description": "schema:description",
        "needs": {
          "@id": "ukiyoue:need",
          "@container": "@set"
        },
        "successCriteria": {
          "@id": "ukiyoue:successCriterion",
          "@container": "@set"
        },
        "priority": "ukiyoue:priorityLevel",
        "influence": "ukiyoue:influenceLevel",
        "interest": "ukiyoue:interestLevel"
      }
    },
    "Requirement": {
      "@id": "ukiyoue:Requirement",
      "@context": {
        "title": "schema:name",
        "description": "schema:description",
        "priority": "ukiyoue:priorityLevel",
        "status": "ukiyoue:requirementStatus",
        "category": "ukiyoue:requirementCategory",
        "acceptanceCriteria": {
          "@id": "ukiyoue:acceptanceCriterion",
          "@container": "@set"
        },
        "dependencies": {
          "@id": "ukiyoue:dependsOn",
          "@type": "@id",
          "@container": "@set"
        },
        "relatedRequirements": {
          "@id": "ukiyoue:relatedTo",
          "@type": "@id",
          "@container": "@set"
        },
        "stakeholders": {
          "@id": "ukiyoue:requestedBy",
          "@type": "@id",
          "@container": "@set"
        }
      }
    },
    "UseCase": {
      "@id": "ukiyoue:UseCase",
      "@context": {
        "title": "schema:name",
        "description": "schema:description",
        "actor": {
          "@id": "ukiyoue:primaryActor",
          "@type": "@id"
        },
        "preconditions": {
          "@id": "ukiyoue:precondition",
          "@container": "@set"
        },
        "postconditions": {
          "@id": "ukiyoue:postcondition",
          "@container": "@set"
        },
        "mainFlow": {
          "@id": "ukiyoue:mainFlow",
          "@container": "@list"
        },
        "alternativeFlows": {
          "@id": "ukiyoue:alternativeFlow",
          "@container": "@set"
        },
        "exceptionFlows": {
          "@id": "ukiyoue:exceptionFlow",
          "@container": "@set"
        },
        "relatedRequirements": {
          "@id": "ukiyoue:addresses",
          "@type": "@id",
          "@container": "@set"
        },
        "priority": "ukiyoue:priorityLevel",
        "frequency": "ukiyoue:executionFrequency"
      }
    },
    "relationships": {
      "@id": "ukiyoue:relationship",
      "@container": "@set",
      "@context": {
        "dependsOn": {
          "@id": "ukiyoue:dependsOn",
          "@type": "@id"
        },
        "relatedTo": {
          "@id": "ukiyoue:relatedTo",
          "@type": "@id"
        },
        "implements": {
          "@id": "ukiyoue:implements",
          "@type": "@id"
        },
        "derivedFrom": {
          "@id": "ukiyoue:derivedFrom",
          "@type": "@id"
        },
        "supersedes": {
          "@id": "ukiyoue:supersedes",
          "@type": "@id"
        },
        "supersededBy": {
          "@id": "ukiyoue:supersededBy",
          "@type": "@id"
        },
        "references": {
          "@id": "ukiyoue:references",
          "@type": "@id"
        },
        "referencedBy": {
          "@id": "ukiyoue:referencedBy",
          "@type": "@id"
        }
      }
    },
    "metadata": {
      "@id": "ukiyoue:metadata",
      "@context": {
        "version": "schema:version",
        "status": "ukiyoue:documentStatus",
        "created": {
          "@id": "dc:created",
          "@type": "xsd:dateTime"
        },
        "modified": {
          "@id": "dc:modified",
          "@type": "xsd:dateTime"
        },
        "authors": {
          "@id": "dc:creator",
          "@container": "@set"
        },
        "tags": {
          "@id": "dc:subject",
          "@container": "@set"
        },
        "language": "dc:language",
        "audience": {
          "@id": "dc:audience",
          "@container": "@set"
        },
        "confidentiality": "ukiyoue:confidentialityLevel"
      }
    },
    "priorityLevel": {
      "@id": "ukiyoue:priorityLevel",
      "@type": "@vocab"
    },
    "documentStatus": {
      "@id": "ukiyoue:documentStatus",
      "@type": "@vocab"
    },
    "requirementStatus": {
      "@id": "ukiyoue:requirementStatus",
      "@type": "@vocab"
    }
  }
}
